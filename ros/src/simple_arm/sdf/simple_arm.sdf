<?xml version='1.0'?>
<sdf version='1.6'>

    <model name="simple_arm">

        <link name="arm_footprint">
            <must_be_base_link>1</must_be_base_link>
            <self_collide>1</self_collide>
            <pose>0.0 0 0 0 0.0 0</pose>
            <inertial>
                <inertia>
                    <ixx>100</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>100</iyy>
                    <iyz>0</iyz>
                    <izz>100</izz>
                </inertia>
                <mass>.1</mass>
            </inertial>

            <visual name='arm_footprint_visual'>
                <geometry>
                    <box>
                        <size>0.15 .095 .003</size>
                    </box>
                </geometry>
            </visual>
            <collision name='arm_footprint_collission'>
                <geometry>
                    <box>
                        <size>0.15 .095 .003</size>
                    </box>
                </geometry>
            </collision>

        </link>
        <!-- begin base block -->
        <joint name="arm_footprint_base_block_joint" type="fixed">
            <pose>-0.035 0 0.003</pose>
            <child>base_block</child>
            <parent>arm_footprint</parent>
        </joint>

        <link name='base_block'>
            <inertial>
                <mass>.05</mass>
            </inertial>

            <self_collide>1</self_collide>
            <pose >-0.035 0 0.0125 0 0 0</pose>
            <collision name='base_block_collision'>
                <geometry>
                    <box>
                        <size>0.06 0.06 0.028</size>
                    </box>
                </geometry>
            </collision>

            <visual name='base_block_visual'>
                <material>
                    <ambient>0.1 0.1 0.1 1</ambient>
                    <diffuse>0.1 0.1 0.2 1</diffuse>
                    <specular>0 0 0 0</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
                <geometry>
                    <box>
                        <size>0.06 0.06 0.028</size>
                    </box>
                </geometry>
            </visual>

        </link>
        <!-- end base block -->
        <!-- begin rotating block -->
        <joint name="base_block_rotating_block_joint" type="revolute">
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
            <!--<pose>-0.035 0 0.25 0 0 0</pose>-->
            <pose>0 0 0  0 0 0</pose>
            <child>rotating_block</child>
            <parent>base_block</parent>
        </joint>
        <link name='rotating_block'>
            <inertial>
                <mass>.05</mass>
            </inertial>

            <self_collide>1</self_collide>
            <pose >-0.035 0 0.03 0 0 0</pose>
            <collision name='rotating_block_collision_right_raised'>
                <pose>-0.023 -0.0285 0.045 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.01 0.003 0.01</size>
                    </box>
                </geometry>
            </collision>
            <visual name='rotating_block_visual_right_raised'>
                <pose>-0.023 -0.0285 0.045 0 0 0</pose>
                <material>
                    <ambient>0.1 0.1 0.1 1</ambient>
                    <diffuse>0.1 0.1 0.2 1</diffuse>
                    <specular>0 0 0 0</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
                <geometry>
                    <box>
                        <size>0.01 0.003 0.01</size>
                    </box>
                </geometry>
            </visual>

            <collision name='rotating_block_collision_right'>
                <pose>0 -0.0285 0.02 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.06 0.003 0.04</size>
                    </box>
                </geometry>
            </collision>

            <visual name='rotating_block_visual_right'>
                <pose>0 -0.0285 0.02 0 0 0</pose>

                <geometry>
                    <box>
                        <size>0.06 0.003 0.04</size>
                    </box>
                </geometry>
            </visual>
            <collision name='rotating_block_collision_left'>
                <pose>0 0.0285 0.02 0 0 0</pose>

                <geometry>
                    <box>
                        <size>0.06 0.003 0.04</size>
                    </box>
                </geometry>
            </collision>
            <visual name='rotating_block_visual_left'>
                <pose>0 0.0285 0.02 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.06 0.003 0.04</size>
                    </box>
                </geometry>
            </visual>
            <collision name='rotating_block_collision_base'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.06 0.06 0.003</size>
                    </box>
                </geometry>
            </collision>
            <visual name='rotating_block_visual_base'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.06 0.06 0.003</size>
                    </box>
                </geometry>
            </visual>

            <visual name='rotating_block_vertical_arm_axis'>
                <!-- no collission for this part as it's just for our visual reference -->
                <pose>0.020 0 0.01925 1.573301 0 0</pose>
                <material>
                    <ambient>0.1 0.1 0.1 1</ambient>
                    <diffuse>0.1 0.1 0.2 1</diffuse>
                    <specular>0 0 0 0</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
                <geometry>
                    <cylinder>
                        <length>0.06</length>
                        <radius>0.005</radius>
                    </cylinder>
                </geometry>
            </visual>

        </link>
        <!-- end rotating block -->
        <!-- begin vertical arm -->

        <joint name="rotating_block_vertical_arm_elbow_joint" type="revolute">
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
            <pose>-0.040 0 0 0 0 0</pose>
            <child>vertical_arm</child>
            <parent>rotating_block</parent>
        </joint>

        <link name='vertical_arm'>
            <inertial>
                <mass>.05</mass>
            </inertial>

            <self_collide>1</self_collide>
            <pose >0.0265 -0.004 0.05125 0 0 0</pose>
            <collision name='vertical_arm_collision'>
                <geometry>
                    <box>
                        <!-- collission stops just before the ends to accomodate the joints-->
                        <size>0.012 0.025 0.083 </size>
                    </box>
                </geometry>
            </collision>

            <visual name='vertical_arm_visual'>
                <geometry>
                    <box>
                        <size>0.012 0.025 0.093</size>
                    </box>
                </geometry>
            </visual>

        </link>
        <!-- end vertical arm -->
        <!-- begin link arm -->

        <joint name="rotating_block_link_arm_elbow_joint" type="revolute">
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
            <pose>-0.04375 0 0 0 0 0</pose>
            <child>link_arm</child>
            <parent>rotating_block</parent>
        </joint>
        <joint name="rotating_block_vertical_arm_elbow_joint" type="revolute">
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
            <pose>-0.04375 0 0 0 0 0</pose>
            <child>verticalarm</child>
            <parent>rotating_block</parent>
        </joint>

        <link name='link_arm'>
            <inertial>
                <mass>.05</mass>
            </inertial>

            <self_collide>1</self_collide>
            <pose >.067 -0.025 0.076 0 0 0</pose>
            <collision name='link_arm_collision'>
                <geometry>
                    <box>
                        <!-- collission stops just before the ends to accomodate the joints-->
                        <size>0.003 0.00785 0.080</size>
                    </box>
                </geometry>
            </collision>

            <visual name='link_arm_visual'>
                <geometry>
                    <box>
                        <size>0.003 0.00785 0.0875</size>
                    </box>
                </geometry>
            </visual>

        </link>
        <!-- end link arm -->

        <!-- begin top_link -->

        <joint name="top_link_link_arm_joint" type="revolute">
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
            <pose>0.067 0.00785 0.0875 0 0 0</pose>
            <child>link_arm</child>
            <parent>top_link</parent>
        </joint>
        <joint name="top_link_vertical_arm_joint" type="revolute">
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
            <pose>-0.053 0 0 0 0 0</pose>
            <child>vertical_arm</child>
            <parent>top_link</parent>
        </joint>

        <link name='top_link'>
            <inertial>
                <mass>.05</mass>
            </inertial>

            <self_collide>1</self_collide>
            <pose >-0.0205 -0.028 0.076 1.573 0 0</pose>
            <collision name='toip_link_collision'>
                <geometry>
                    <box>
                        <!-- collission stops just before the ends to accomodate the joints-->
                        <size>0.080 0.003 0.00785 </size>
                    </box>
                </geometry>
            </collision>

            <visual name='top_link_visual'>
                <geometry>
                    <polyline>
                        <point>0 0</point>
                        <point>0.036 -0.015</point>
                        <point>0.048 0</point>
                        <height>.003</height>
                    </polyline>

                </geometry>
            </visual>

        </link>
        <!-- end link arm -->

    </model>
</sdf>